print = __builtin_print;



d_o = () => {
    g = 10;
    outer = (a, b) => {
        a = 1;
        x = 2;

        inner = () => {
            print(g);
            print(a);
            print(b);
            print(x);
            a = 10;
            b = 20;
        };

        .: a = 3;
        .: x = 4;

        inner();

        inner;
    };

    outer;
};


d_o()(0, 10)();
.: outer(0, 10)();